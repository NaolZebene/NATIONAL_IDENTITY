
 <% layout('boilerplate') %>

 <div class="container-fluid">
   <h3 class="text-dark mb-1">Add Request</h3>
 </div>
 
 <form class="shadow-sm" style="margin: 38px;margin-top: 29px;padding: 26px;" action="/requests" method="post">
   <input class="form-control" name="idNumber" type=s"text" placeholder="Id Number">

   <select class="form-control" style="margin-top: 36px;" name="priority" required>
