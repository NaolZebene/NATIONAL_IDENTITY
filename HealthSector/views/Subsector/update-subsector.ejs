
<% layout('boilerplate') %>

<div class="container-fluid">
  <h3 class="text-dark mb-1">Update Subsector</h3>
</div>

<form class="shadow-sm" style="margin: 38px;margin-top: 29px;padding: 26px;" action="/subsectors/update/<%=subsector._id%>" method="post">

  <input class="form-control" name="name" value="<%= subsector.name %>" type="text" placeholder="Name" style="margin-top: 39px;">

  <input class="form-control" name="location" value="<%= subsector.location %>" type="text" placeholder="Location" style="margin-top: 39px;">

  <input class="form-control" name="email" value="<%= subsector.email %>" type="text" placeholder="Email" style="margin-top: 39px;">

  <input class="form-control" name="username" value="<%= subsector.username %>" type="text" placeholder="Username" style="margin-top: 39px;">

  
  <button  class="btn btn-primary" data-bs-hover-animate="pulse"  type="submit" style="margin-top: 40px;"><i class="fa fa-plus"></i>&nbsp;Submit</button>
</form>

</div>
</div>
<a class="border rounded d-inline scroll-to-top" href="#page-top"><i class="fas fa-angle-up"></i>
</a>
</div>

