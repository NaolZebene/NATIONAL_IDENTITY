<h1>Create Weapon</h1>

<form action="/weapons/create" method="POST" enctype="multipart/form-data">
  <label for="weaponName">Weapon Name</label>
  <input type="text" id="weaponName" name="weaponName" required>

  <label for="weaponAmount">Weapon Amount</label>
  <input type="number" id="weaponAmount" name="weaponAmount" required>

  <label for="weaponSerialNumber">Weapon Serial Number</label>
  <input type="text" id="weaponSerialNumber" name="weaponSerialNumber" required>

  <label for="additionalFiles">Additional Files</label>
  <input type="file" id="additionalFiles" name="additionalFiles" multiple>

  <button type="submit">Create</button>
</form>
    